{
    "name": "webtrend/cookie-set",
    "description": "A plugin that stores Google Analytics utm_parameter query string to a client cookie.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "cookie set"
    ],
    "support": {
        "docs": "https://github.com/webtrend/cookieset/blob/master/README.md",
        "issues": "https://github.com/webtrend/cookieset/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Webtrend",
            "homepage": "http://"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "webtrend\\cookieset\\": "src/"
        }
    },
    "extra": {
        "name": "Cookie Set",
        "handle": "cookie-set",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/webtrend/cookieset/blob/master/CHANGELOG.md",
        "class": "webtrend\\cookieset\\CookieSet"
    }
}
